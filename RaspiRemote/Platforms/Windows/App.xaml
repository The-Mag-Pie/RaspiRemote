<maui:MauiWinUIApplication
    x:Class="RaspiRemote.WinUI.App"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:maui="using:Microsoft.Maui"
    xmlns:local="using:RaspiRemote.WinUI">

    <maui:MauiWinUIApplication.Resources>

        <SolidColorBrush x:Key="ComboBoxForegroundPressed" Color="Black" />
        <SolidColorBrush x:Key="ComboBoxForegroundPointerOver" Color="Black" />
        <SolidColorBrush x:Key="ComboBoxForegroundDisabled" Color="Gray" />
        <SolidColorBrush x:Key="ComboBoxDropDownGlyphForeground" Color="Black" />
        <SolidColorBrush x:Key="ComboBoxDropDownGlyphForegroundDisabled" Color="Gray" />

        <x:Double x:Key="ProgressBarTrackHeight">5</x:Double>

    </maui:MauiWinUIApplication.Resources>

</maui:MauiWinUIApplication>
